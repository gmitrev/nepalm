<% if @project && @project.archived? %>
  <div class="alert alert-warning fade in" style='margin-top: 15px'>
    <i class="fa fa-warning"></i> This project is archived.
  </div>
<% elsif @stack && @stack.archived? %>
  <div class="alert alert-warning fade in" style='margin-top: 15px'>
    <i class="fa fa-warning"></i> This stack is archived.
  </div>
<% end %>
